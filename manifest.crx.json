{
    "manifest_version": 3,
    "name": "Ecsta",
    "short_name": "Ecsta",
    "version": "0.3.4",
  
    "description": "A simple new tab extension meant to stay out of your way while still making for great eye-candy.",

    "icons": {
        "32": "./res/icons/32.png",
        "48": "./res/icons/48.png",
        "64": "./res/icons/64.png",
        "128": "./res/icons/128.png",
        "256": "./res/icons/256.png"
    },

    "chrome_url_overrides": {
        "newtab": "./index.html"
    }
}
  